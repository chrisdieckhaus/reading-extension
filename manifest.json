{
  "manifest_version": 2,

  "name": "Article Saver",
  "description": "Save articles to read later",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": ""
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}